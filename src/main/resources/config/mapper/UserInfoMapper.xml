<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="techone.blogging.persistence.UserInfoMapper" >
  <resultMap id="BaseResultMap" type="techone.blogging.domain.entity.UserInfoEntity" >
    <id column="id" property="id"/>
    <result column="member_id" property="memberId"/>
  </resultMap>
  <sql id="Base_Column_List" >
    id, member_id
  </sql>
  <insert id="insertSelective" parameterType="techone.blogging.domain.entity.UserInfoEntity">
    insert into user_info
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="memberId != null">
        member_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id},
      </if>
      <if test="memberId != null">
        #{memberId},
      </if>
    </trim>
  </insert>

  <select id="selectTest" resultMap="BaseResultMap">
    select * from user_info;
  </select>
</mapper>